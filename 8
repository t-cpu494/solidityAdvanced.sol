pragma solidity >= 0.5.0 < 0.9.0;

contract ArrayElementsSum {
    int[] public contractStorage;
    function UserInputArray(int[] calldata userInputArray) public {
        contractStorage = userInputArray;
    }

    function arrayElementsSum() public view returns(int) {
        int sum = 0;
        for(uint i = 0; i < contractStorage.length; i++) {
            sum += contractStorage[i];
        }
        return sum;
    }
}
