pragma solidity >= 0.5.0 < 0.9.0;

contract Reverse {
    int[] public contractStorage;
    function reverse(int[] memory userInputArray) public {
        contractStorage = new int[](userInputArray.length);
        for(uint i = 0; i < userInputArray.length; i++) {
            contractStorage[i] = userInputArray[userInputArray.length - i - 1];
        }
    }
}
